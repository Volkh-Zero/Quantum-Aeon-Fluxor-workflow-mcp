  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
